{
  "name": "aby-blog-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8082",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "build-and-copy": "npm run build && npm run copy-to-springboot",
    "copy-to-springboot": "xcopy /E /Y dist/* ../AbyBlog-app/src/main/resources/static/"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "axios": "^1.6.1",
    "core-js": "^3.8.3",
    "element-plus": "^2.4.3",
    "g": "^2.0.1",
    "glob": "^10.3.10",
    "mitt": "^3.0.1",
    "node-sass": "^9.0.0",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "sass": "^1.69.5",
    "vue": "^3.3.9",
    "vue-router": "^4.2.5",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@types/body-parser": "^1.19.5",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/compiler-sfc": "^3.3.9",
    "eslint": "^8.53.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-vue": "^9.18.1",
    "sass-loader": "^13.3.2",
    "ts-loader": "^9.5.1",
    "vue-loader": "^17.4.2",
    "vue-template-compiler": "^2.7.16",
    "webpack-cli": "^5.1.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
